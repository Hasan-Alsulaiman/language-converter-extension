{
    "name": "Arabic-Persian",
    "description": "fool the machine?!",
    "version": "1.0",
    "manifest_version": 3,
    "icons": { "16": "icon16.png",
      "48": "icon48.png",
     "128": "128.png" },
    "permissions": ["storage", "activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "web_accessible_resources": [
      { 
        "resources": ["/json/json.json"],
        "matches": ["<all_urls>"]
      }
    ]
  }